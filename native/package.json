{
  "name": "manifold",
  "version": "1.0.0",
  "scripts": {
    "ios:dev": "NEXT_PUBLIC_FIREBASE_ENV=DEV yarn ios -d",
    "ios:prod": "NEXT_PUBLIC_FIREBASE_ENV=PROD yarn ios -d",
    "android:dev": "NEXT_PUBLIC_FIREBASE_ENV=DEV yarn android -d",
    "prebuild": "npx expo prebuild",
    "clean": "npx expo prebuild --clean",
    "build:dev": "yarn prebuild && NEXT_PUBLIC_FIREBASE_ENV=DEV sh envscript.sh && eas build --local",
    "build:preview": "yarn prebuild && NEXT_PUBLIC_FIREBASE_ENV=PROD sh envscript.sh && eas build --local",
    "build:prod": "yarn prebuild && NEXT_PUBLIC_FIREBASE_ENV=PROD sh envscript.sh && eas build --local",
    "build:android:dev": "yarn build:dev -p android --profile preview",
    "build:android:preview": "yarn build:preview -p android --profile preview",
    "build:android:prod": "yarn build:prod -p android --profile production ",
    "build:ios:dev": "yarn build:dev -p ios --profile preview",
    "build:ios:preview": "yarn build:preview -p ios --profile preview",
    "build:ios:prod": "yarn build:prod -p ios --profile production",
    "android:prod": "NEXT_PUBLIC_FIREBASE_ENV=PROD yarn android -d",
    "clear": "expo start --clear",
    "start": "expo start --dev-client",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@sentry/react-native": "4.2.2",
    "@types/react-native": "~0.69.1",
    "expo": "~46.0.17",
    "expo-apple-authentication": "~4.3.0",
    "expo-application": "~4.2.2",
    "expo-auth-session": "~3.7.1",
    "expo-constants": "~13.2.4",
    "expo-dev-client": "^1.3.1",
    "expo-device": "~4.3.0",
    "expo-notifications": "^0.16.1",
    "expo-random": "~12.3.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-updates": "~0.14.7",
    "expo-web-browser": "~11.0.0",
    "firebase": "^9.12.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-iap": "^12.4.2",
    "react-native-web": "~0.18.7",
    "react-native-webview": "11.23.0",
    "sentry-expo": "~5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4"
  },
  "private": true
}
